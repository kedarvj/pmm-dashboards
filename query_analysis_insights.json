      "pluginVersion": "11.6.4",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${DS_CLICKHOUSE}"
          },
          "editorType": "sql",
          "format": 0,
          "meta": {
            "builderOptions": {
              "columns": [],
              "database": "",
              "limit": 1000,
              "mode": "list",
              "queryType": "table",
              "table": ""
            }
          },
          "pluginVersion": "4.10.2",
          "queryType": "timeseries",
          "rawSql": "SELECT\n  period_start AS time,\n  sum(m_query_time_sum / m_query_time_cnt) AS query_time,\n  fingerprint AS query_text\nFROM pmm.metrics\nWHERE\n  service_type = 'mysql'\n  AND (service_name = '' OR service_name = '$service_name')\n  AND $__timeFilter(period_start)\n  AND fingerprint IN (\n    SELECT fingerprint\n    FROM pmm.metrics\n    WHERE service_type = 'mysql'\n      AND $__timeFilter(period_start)\n      AND (service_name = '' OR service_name = '$service_name')\n    GROUP BY fingerprint\n    ORDER BY sum(m_query_time_sum) DESC\n    LIMIT 10\n  )\nGROUP BY time, fingerprint\nORDER BY time, query_time DESC;\n",
          "refId": "A"
        }
      ],
      "title": "Query Analysis - $service_name",
      "type": "timeseries"
    },
